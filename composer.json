{
    "name": "netfantom/yii2-robokassa",
    "type": "yii2-extension",
    "description": "Robokassa Yii2 Framework extension for PHP 8.1",
    "keywords": [
        "robokassa",
        "yii2",
        "widget",
        "payment"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Fedorov",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "netFantom\\Yii2Robokassa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "netfantom/robokassa-api": "@dev",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-httpclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^5.12"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
